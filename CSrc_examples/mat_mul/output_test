void matmul(int **A, int **B, int **C, int matHeight, int matWidth) {
  int sum;
  int   i;
  int   j;
  int   k;
  int mulTemp; 
  
//------calculating element 0 and 0
sum = 0;
mulTemp = A[0][0]*B[0][0];
sum = sum + mulTemp;
mulTemp = A[0][1]*B[1][0];
sum = sum + mulTemp;
mulTemp = A[0][2]*B[2][0];
sum = sum + mulTemp;
C[0][0] = sum;
//------calculating element 0 and 1
sum = 0;
mulTemp = A[0][0]*B[0][1];
sum = sum + mulTemp;
mulTemp = A[0][1]*B[1][1];
sum = sum + mulTemp;
mulTemp = A[0][2]*B[2][1];
sum = sum + mulTemp;
C[0][1] = sum;
//------calculating element 0 and 2
sum = 0;
mulTemp = A[0][0]*B[0][2];
sum = sum + mulTemp;
mulTemp = A[0][1]*B[1][2];
sum = sum + mulTemp;
mulTemp = A[0][2]*B[2][2];
sum = sum + mulTemp;
C[0][2] = sum;
//------calculating element 1 and 0
sum = 0;
mulTemp = A[1][0]*B[0][0];
sum = sum + mulTemp;
mulTemp = A[1][1]*B[1][0];
sum = sum + mulTemp;
mulTemp = A[1][2]*B[2][0];
sum = sum + mulTemp;
C[1][0] = sum;
//------calculating element 1 and 1
sum = 0;
mulTemp = A[1][0]*B[0][1];
sum = sum + mulTemp;
mulTemp = A[1][1]*B[1][1];
sum = sum + mulTemp;
mulTemp = A[1][2]*B[2][1];
sum = sum + mulTemp;
C[1][1] = sum;
//------calculating element 1 and 2
sum = 0;
mulTemp = A[1][0]*B[0][2];
sum = sum + mulTemp;
mulTemp = A[1][1]*B[1][2];
sum = sum + mulTemp;
mulTemp = A[1][2]*B[2][2];
sum = sum + mulTemp;
C[1][2] = sum;
//------calculating element 2 and 0
sum = 0;
mulTemp = A[2][0]*B[0][0];
sum = sum + mulTemp;
mulTemp = A[2][1]*B[1][0];
sum = sum + mulTemp;
mulTemp = A[2][2]*B[2][0];
sum = sum + mulTemp;
C[2][0] = sum;
//------calculating element 2 and 1
sum = 0;
mulTemp = A[2][0]*B[0][1];
sum = sum + mulTemp;
mulTemp = A[2][1]*B[1][1];
sum = sum + mulTemp;
mulTemp = A[2][2]*B[2][1];
sum = sum + mulTemp;
C[2][1] = sum;
//------calculating element 2 and 2
sum = 0;
mulTemp = A[2][0]*B[0][2];
sum = sum + mulTemp;
mulTemp = A[2][1]*B[1][2];
sum = sum + mulTemp;
mulTemp = A[2][2]*B[2][2];
sum = sum + mulTemp;
C[2][2] = sum;
}
